<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.shape.Line?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="AnchorP" prefHeight="400.0" prefWidth="600.0" stylesheets="@styles.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="datastruct.le.ConfirmationUI">
    <children>
        <Label fx:id="TaskPromp" layoutX="24.0" layoutY="22.0" text="Confirmation Prompt">
            <font>
                <Font name="System Bold" size="24.0" />
            </font>
        </Label>
        <Line endX="499.0" layoutX="115.0" layoutY="73.0" startX="-115.0" />
        <Label fx:id="AYS" layoutX="128.0" layoutY="145.0" text="Are You Sure?">
            <font>
                <Font name="System Bold" size="36.0" />
            </font>
        </Label>
        <Button fx:id="ConYes" layoutX="89.0" layoutY="280.0" mnemonicParsing="false" prefHeight="50.0" prefWidth="200.0" style="-fx-background-radius: 20;" text="Yes">
            <font>
                <Font size="24.0" />
            </font>
        </Button>
        <Button fx:id="ConNo" layoutX="341.0" layoutY="280.0" mnemonicParsing="false" prefHeight="50.0" prefWidth="200.0" style="-fx-background-radius: 20;" text="No">
            <font>
                <Font size="24.0" />
            </font>
        </Button>
    </children>
</AnchorPane>
